{
    "workbench.colorTheme": "Blueberry Banana",
    "editor.renderControlCharacters": true,
    "window.zoomLevel": 0,
    "editor.fontFamily": "Fira Code, Menlo",
    "editor.renderWhitespace": "none",
    "editor.fontLigatures": true,
    "python.pythonPath": "/usr/local/bin/python3",
